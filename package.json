{
    "name": "ttx_1.app",
    "version": "1.0.0",
    "private": true,
    "description": "災害向けTTX支援システム - 恵那市を対象とした机上訓練支援Webアプリケーション",
    "scripts": {
      "dev": "npm-run-all -p dev:front dev:back",
      "dev:front": "npm --prefix frontend run dev",
      "dev:back": "npm --prefix backend run dev",
      "build": "npm --prefix frontend run build && npm --prefix backend run build",
      "start": "npm --prefix backend start"
    },
    "devDependencies": {
      "npm-run-all": "^4.1.5"
    },
    "author": "植木 崇斗（Hosei University）",
    "license": "MIT",
    "keywords": [
      "TTX",
      "Disaster",
      "Ena City",
      "Leaflet",
      "OpenStreetMap",
      "React",
      "LowDB"
    ]
  }
  